{
    "name": "fp/route-kit-dev",
    "description": "Development beta combining route-kit core and CLI features. Intended for testing and feedback before splitting into modules.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Francisco Paz",
            "email": "josepaz3123@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/routing": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "spatie/laravel-permission": "^5.5|^6.0|^7.0",
        "laravel/prompts": "^0.1.24|^0.2|^0.3",
        "laravel/pint": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^9.14"  
    },
    "autoload": {
        "psr-4": {
            "Fp\\FullRoute\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Fp\\FullRoute\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fp\\FullRoute\\FullRouteServiceProvider"
            ],
            "aliases": {
                "Fp": "Fp\\FullRoute\\Facades\\Fp"
            }
        }
    },
    "scripts": {
        "test": "phpunit --colors=always --display-deprecations",
        "format": "pint"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
